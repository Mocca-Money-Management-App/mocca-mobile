<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_navigation"
    app:startDestination="@id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.bangkit.mocca.ui.home.HomeFragment"
        android:label="navigation_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_budget"
        android:name="com.bangkit.mocca.ui.budget.BudgetFragment"
        android:label="navigation_budget"
        tools:layout="@layout/fragment_budget" />

    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.bangkit.mocca.ui.profile.ProfileFragment"
        android:label="navigation_profile"
        tools:layout="@layout/fragment_profile" />

    <fragment
        android:id="@+id/navigation_report"
        android:name="com.bangkit.mocca.ui.report.ReportFragment"
        android:label="navigation_report"
        tools:layout="@layout/fragment_report" />
</navigation>